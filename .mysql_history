show databases;
use mysql;
show tables;
create database hell;
use hell
create table hell1 (id int(5),name char(20), sal int(10));
show table hell1;
show create table hell1;
insert into hell1 values (1,'vinay',2);
insert into hell1 values (2,'vinayk',3);
select * from hell1;
show tables;
create table hell2 (name char(20));
show tables;
select * from hell2;
drop table hell2;
show tables;
select * from hell2;
